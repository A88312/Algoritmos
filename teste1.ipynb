   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def minMin(tabMeteo):\n",
    "    min = tabMeteo[0][1]\n",
    "    for reg in tabMeteo:\n",
    "        _, tmin, _, _ = reg\n",
    "        if tmin < min:\n",
    "            min = tmin \n",
